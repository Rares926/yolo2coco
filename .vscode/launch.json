        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "Yolo to Coco",
            "type": "python",
            "request": "launch",
            "module": "main",
            "justMyCode": true,
            "args":[
                "--path",
                "C:/ML/workspace_new/detr/small_for_testing/yolo",
                "--output",
                "C:/ML/workspace_new/detr/small_for_testing/coco",
                "-cpy"
            ]
        }
    ]
}